[Unit]
Description=EarnApp auto-upgrader
Requires=network-online.target
After=network.target network-online.target

[Service]
Type=simple
User=root
Group=root
ExecStart=/root/.local/bin/earnapp autoupgrade
Nice=-20
Restart=always
RestartSec=5
StandardOutput=null
StandardError=null

[Install]
WantedBy=multi-user.target
